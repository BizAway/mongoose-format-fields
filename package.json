{
  "name": "mongoose-format-fields",
  "description": "A plugin for Mongoose that formats the output of a model depending on the tags associated",
  "version": "0.5.0",
  "author": "Flavio Del Bianco <info@flaviodelbianco.com>",
  "main": "index.js",
  "homepage": "https://github.com/BizAway/mongoose-format-fields",
  "scripts": {
    "test": "mocha ./test/*.js",
    "test-travis": "npm run linter && nyc --reporter=lcov npm test && nyc check-coverage --lines 80 --functions 80 --branches 80",
    "linter-autofix": "standard --fix",
    "linter": "standard"
  },
  "keywords": [
    "mongodb",
    "mongoose",
    "plugin",
    "format",
    "fields",
    "tags"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BizAway/mongoose-format-fields.git"
  },
  "dependencies": {},
  "license": "MIT",
  "devDependencies": {
    "mocha": "^8.1.3",
    "mongoose": "5.x",
    "nyc": "11.7.3",
    "standard": "^14.3.4"
  },
  "peerDependencies": {
    "mongoose": "4.x || 5.x"
  },
  "bugs": {
    "url": "https://github.com/BizAway/mongoose-format-fields/issues"
  },
  "standard": {
    "ignore": [
      "**/test/"
    ]
  }
}
